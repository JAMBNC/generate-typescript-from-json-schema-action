{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "$defs" : {
    "Address" : {
      "type" : "object",
      "properties" : {
        "city" : {
          "type" : "string"
        },
        "company" : {
          "type" : "string"
        },
        "country" : {
          "type" : "string"
        },
        "firstName" : {
          "type" : "string"
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "lastName" : {
          "type" : "string"
        },
        "phone" : {
          "type" : "string"
        },
        "postal" : {
          "type" : "string"
        },
        "region" : {
          "type" : "string"
        },
        "streets" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        }
      },
      "required" : [ "city", "streets" ]
    },
    "CategoryReference" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "integer"
        },
        "label" : {
          "type" : "string"
        },
        "url" : {
          "type" : "string"
        }
      },
      "required" : [ "id", "label", "url" ]
    },
    "Customer" : {
      "type" : "object",
      "properties" : {
        "addresses" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Address"
          }
        },
        "email" : {
          "type" : "string"
        },
        "firstName" : {
          "type" : "string"
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "lastName" : {
          "type" : "string"
        }
      }
    },
    "Discount" : {
      "type" : "object",
      "properties" : {
        "amount" : {
          "type" : "number",
          "minimum" : 0
        },
        "code" : {
          "type" : "string"
        },
        "description" : {
          "type" : "string"
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        }
      }
    },
    "LineItem" : {
      "type" : "object",
      "properties" : {
        "children" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/LineItem"
          }
        },
        "discountAmount" : {
          "type" : "number",
          "minimum" : 0
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "imageUrls" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "name" : {
          "type" : "string"
        },
        "productIdentifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "quantity" : {
          "type" : "number",
          "exclusiveMinimum" : 0
        },
        "sku" : {
          "type" : "string"
        },
        "subtotal" : {
          "type" : "number",
          "minimum" : 0
        },
        "taxAmount" : {
          "type" : "number",
          "minimum" : 0
        },
        "total" : {
          "type" : "number",
          "minimum" : 0
        },
        "weightInPounds" : {
          "type" : "number",
          "minimum" : 0
        }
      },
      "required" : [ "name", "quantity", "sku", "subtotal", "taxAmount", "total", "weightInPounds" ]
    },
    "Map(String,Object)" : {
      "type" : "object"
    },
    "MediaType" : {
      "type" : "string",
      "enum" : [ "Image", "Video" ]
    },
    "Method" : {
      "type" : "string",
      "enum" : [ "CASH", "PO", "CREDIT_CARD", "ON_ACCOUNT" ]
    },
    "NonNegativeIntRange" : {
      "type" : "object",
      "properties" : {
        "max" : {
          "type" : "integer",
          "minimum" : 0
        },
        "min" : {
          "type" : "integer",
          "minimum" : 0
        }
      },
      "required" : [ "max", "min" ]
    },
    "Order" : {
      "type" : "object",
      "properties" : {
        "billingAddress" : {
          "$ref" : "#/$defs/Address"
        },
        "comments" : {
          "type" : "string"
        },
        "currencyCode" : {
          "type" : "string"
        },
        "customerIdentifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "discounts" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Discount"
          }
        },
        "email" : {
          "type" : "string"
        },
        "firstName" : {
          "type" : "string"
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "lastName" : {
          "type" : "string"
        },
        "lineItems" : {
          "minItems" : 1,
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/LineItem"
          }
        },
        "orderDate" : {
          "type" : "string",
          "format" : "date-time"
        },
        "orderNumber" : {
          "type" : "string",
          "description" : "Globally unique orderNumber.  Usually a combination of sales channel and order number",
          "minLength" : 1,
          "maxLength" : 50
        },
        "payments" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Payment"
          }
        },
        "salesChannel" : {
          "type" : "string"
        },
        "shippingAddress" : {
          "$ref" : "#/$defs/Address"
        },
        "shippingAmount" : {
          "type" : "number",
          "minimum" : 0
        },
        "shippingInfo" : {
          "$ref" : "#/$defs/ShippingInfo"
        },
        "status" : {
          "$ref" : "#/$defs/Status"
        },
        "subtotal" : {
          "type" : "number",
          "minimum" : 0
        },
        "taxAmount" : {
          "type" : "number",
          "minimum" : 0
        },
        "total" : {
          "type" : "number",
          "minimum" : 0
        }
      },
      "required" : [ "billingAddress", "lineItems", "orderNumber", "salesChannel", "shippingAmount", "subtotal", "taxAmount", "total" ]
    },
    "Payment" : {
      "type" : "object",
      "properties" : {
        "amount" : {
          "type" : "number",
          "minimum" : 0
        },
        "billToName" : {
          "type" : "string"
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "method" : {
          "$ref" : "#/$defs/Method"
        },
        "paymentDate" : {
          "type" : "string",
          "format" : "date-time"
        },
        "poNumber" : {
          "type" : "string"
        },
        "status" : {
          "type" : "string"
        }
      },
      "required" : [ "method" ]
    },
    "Product" : {
      "type" : "object",
      "properties" : {
        "badges" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/ProductBadge"
          }
        },
        "brand" : {
          "type" : "string"
        },
        "browsable" : {
          "type" : "boolean"
        },
        "content" : {
          "$ref" : "#/$defs/ProductContent"
        },
        "createdAt" : {
          "type" : "string",
          "format" : "date-time"
        },
        "customization" : {
          "$ref" : "#/$defs/ProductCustomization"
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "media" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/ProductMedia"
          }
        },
        "name" : {
          "type" : "string"
        },
        "primaryCategory" : {
          "$ref" : "#/$defs/CategoryReference"
        },
        "purchaseOptions" : {
          "$ref" : "#/$defs/PurchaseOptions"
        },
        "searchable" : {
          "type" : "boolean"
        },
        "sku" : {
          "type" : "string"
        },
        "status" : {
          "type" : "string"
        },
        "taxonomy" : {
          "$ref" : "#/$defs/Taxonomy"
        },
        "upc" : {
          "type" : "string"
        },
        "updatedAt" : {
          "type" : "string",
          "format" : "date-time"
        },
        "url" : {
          "type" : "string"
        }
      },
      "required" : [ "browsable", "name", "searchable", "sku", "taxonomy", "url" ]
    },
    "ProductBadge" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "integer"
        },
        "imageUrl" : {
          "type" : "string"
        },
        "label" : {
          "type" : "string"
        }
      }
    },
    "ProductContent" : {
      "type" : "object",
      "properties" : {
        "longDescription" : {
          "type" : "string"
        },
        "metaDescription" : {
          "type" : "string"
        },
        "metaKeywords" : {
          "type" : "string"
        },
        "metaTitle" : {
          "type" : "string"
        },
        "seoDescription" : {
          "type" : "string"
        },
        "shortDescription" : {
          "type" : "string"
        },
        "shortName" : {
          "type" : "string"
        }
      }
    },
    "ProductCustomization" : {
      "type" : "object",
      "properties" : {
        "designRequired" : {
          "type" : "boolean"
        },
        "prebuiltConfigurationId" : {
          "type" : "string"
        },
        "prebuiltCustomizable" : {
          "type" : "boolean"
        },
        "prebuiltDesign" : {
          "type" : "boolean"
        }
      }
    },
    "ProductMedia" : {
      "type" : "object",
      "properties" : {
        "altText" : {
          "type" : "string"
        },
        "mediaType" : {
          "$ref" : "#/$defs/MediaType"
        },
        "tags" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Tag"
          }
        },
        "url" : {
          "type" : "string"
        }
      },
      "required" : [ "mediaType", "url" ]
    },
    "Provider" : {
      "type" : "string",
      "enum" : [ "FedEx", "UPS", "USPS", "Other" ]
    },
    "PurchaseOptions" : {
      "type" : "object",
      "properties" : {
        "allowsSample" : {
          "type" : "boolean"
        },
        "customizable" : {
          "type" : "boolean"
        },
        "customizeLeadTimeDayRange" : {
          "$ref" : "#/$defs/NonNegativeIntRange"
        },
        "inStock" : {
          "type" : "boolean"
        },
        "leadTime" : {
          "type" : "string"
        },
        "minCustomizationQty" : {
          "type" : "integer",
          "exclusiveMinimum" : 0
        },
        "minSaleQty" : {
          "type" : "integer",
          "exclusiveMinimum" : 0
        },
        "onSale" : {
          "type" : "boolean"
        },
        "plainLeadTimeDayRange" : {
          "type" : "string"
        },
        "quantityStepIncrements" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/QuantityStepIncrement"
          }
        },
        "stockQty" : {
          "type" : "integer",
          "minimum" : 0
        },
        "suggestedQuantityDisplays" : {
          "type" : "array",
          "items" : {
            "type" : "integer"
          }
        },
        "tierPrices" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/TierPrice"
          }
        },
        "toBeDiscontinued" : {
          "type" : "boolean"
        }
      },
      "required" : [ "allowsSample", "customizable", "inStock", "stockQty", "tierPrices", "toBeDiscontinued" ]
    },
    "QuantityStepIncrement" : {
      "type" : "object",
      "properties" : {
        "requiredStepIncrement" : {
          "type" : "integer"
        },
        "startingAtQty" : {
          "type" : "integer"
        }
      },
      "required" : [ "requiredStepIncrement", "startingAtQty" ]
    },
    "Shipment" : {
      "type" : "object",
      "properties" : {
        "carrier" : {
          "type" : "string"
        },
        "comments" : {
          "type" : "string"
        },
        "currencyCode" : {
          "type" : "string"
        },
        "estimatedDeliveryDate" : {
          "type" : "string",
          "format" : "date-time"
        },
        "identifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "lineItems" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/LineItem"
          }
        },
        "orderIdentifiers" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "orderNumber" : {
          "type" : "string",
          "description" : "Globally unique orderNumber.  Usually a combination of sales channel and order number",
          "minLength" : 1,
          "maxLength" : 50
        },
        "shipmentDate" : {
          "type" : "string",
          "format" : "date-time"
        },
        "trackingNumber" : {
          "type" : "string"
        },
        "trackingUrl" : {
          "type" : "string"
        }
      },
      "required" : [ "orderNumber", "shipmentDate" ]
    },
    "ShippingInfo" : {
      "type" : "object",
      "properties" : {
        "accountNumber" : {
          "type" : "string"
        },
        "accountType" : {
          "type" : "string"
        },
        "isBlindShipping" : {
          "type" : "boolean"
        },
        "method" : {
          "type" : "string"
        },
        "provider" : {
          "$ref" : "#/$defs/Provider"
        },
        "weightInPounds" : {
          "type" : "number",
          "minimum" : 0
        }
      },
      "required" : [ "isBlindShipping", "provider" ]
    },
    "Status" : {
      "type" : "string",
      "const" : "CREATED"
    },
    "Tag" : {
      "type" : "string",
      "enum" : [ "PRIMARY", "SMALL", "THUMBNAIL", "YOUR_LOGO_HERE", "OTHER" ]
    },
    "Taxonomy" : {
      "type" : "object",
      "properties" : {
        "additionalProperties" : {
          "$ref" : "#/$defs/Map(String,Object)"
        },
        "master" : {
          "type" : "string"
        },
        "primary" : {
          "type" : "string"
        },
        "sub" : {
          "type" : "string"
        }
      }
    },
    "TierPrice" : {
      "type" : "object",
      "properties" : {
        "discountReason" : {
          "type" : "string"
        },
        "price" : {
          "type" : "number"
        },
        "salePrice" : {
          "type" : "number"
        },
        "startingAtQty" : {
          "type" : "integer"
        }
      },
      "required" : [ "price", "startingAtQty" ]
    }
  },
  "type" : "object",
  "properties" : {
    "address" : {
      "$ref" : "#/$defs/Address"
    },
    "customer" : {
      "$ref" : "#/$defs/Customer"
    },
    "order" : {
      "$ref" : "#/$defs/Order"
    },
    "product" : {
      "$ref" : "#/$defs/Product"
    },
    "shipment" : {
      "$ref" : "#/$defs/Shipment"
    }
  }
}
