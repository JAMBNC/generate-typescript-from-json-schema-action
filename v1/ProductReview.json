{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "$defs" : {
    "AnsweredQuestion" : {
      "type" : "object",
      "properties" : {
        "choices" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "selected" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "type" : {
          "$ref" : "#/$defs/Type"
        }
      },
      "required" : [ "choices", "type" ]
    },
    "Media" : {
      "type" : "object",
      "properties" : {
        "altText" : {
          "type" : "string"
        },
        "label" : {
          "type" : "string"
        },
        "mediaType" : {
          "$ref" : "#/$defs/MediaType"
        },
        "previewUrl" : {
          "type" : "string"
        },
        "tags" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Tag"
          }
        },
        "url" : {
          "type" : "string"
        }
      },
      "required" : [ "mediaType", "url" ]
    },
    "MediaType" : {
      "type" : "string",
      "enum" : [ "image", "video" ]
    },
    "MerchantResponse" : {
      "type" : "object",
      "properties" : {
        "createdAt" : {
          "type" : "string",
          "format" : "date-time"
        },
        "identifiers" : {
          "type" : "object"
        },
        "location" : {
          "type" : "string"
        },
        "nickname" : {
          "type" : "string"
        },
        "response" : {
          "type" : "string"
        },
        "responseType" : {
          "$ref" : "#/$defs/ResponseType"
        },
        "updatedAt" : {
          "type" : "string",
          "format" : "date-time"
        },
        "votes" : {
          "type" : "object"
        }
      }
    },
    "ResponseType" : {
      "type" : "string",
      "const" : "merchantResponse"
    },
    "ReviewDetail" : {
      "type" : "object",
      "properties" : {
        "answeredQuestions" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/AnsweredQuestion"
          }
        },
        "comments" : {
          "type" : "string"
        },
        "createdAt" : {
          "type" : "string",
          "format" : "date-time"
        },
        "identifiers" : {
          "type" : "object"
        },
        "location" : {
          "type" : "string"
        },
        "maxRating" : {
          "type" : "number"
        },
        "media" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/Media"
          }
        },
        "merchantResponses" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/MerchantResponse"
          }
        },
        "nickname" : {
          "type" : "string"
        },
        "rating" : {
          "type" : "number"
        },
        "sku" : {
          "type" : "string"
        },
        "source" : {
          "$ref" : "#/$defs/ReviewSource"
        },
        "title" : {
          "type" : "string"
        },
        "votes" : {
          "type" : "object"
        }
      },
      "required" : [ "sku" ]
    },
    "ReviewSource" : {
      "type" : "string",
      "const" : "email"
    },
    "ReviewSummary" : {
      "type" : "object",
      "properties" : {
        "displayGroupId" : {
          "type" : "integer"
        },
        "maxRating" : {
          "type" : "number"
        },
        "rating" : {
          "type" : "number"
        },
        "reviewCount" : {
          "type" : "integer"
        },
        "sku" : {
          "type" : "string"
        }
      }
    },
    "Tag" : {
      "type" : "string",
      "enum" : [ "primary", "small", "thumbnail", "yourLogoHere", "insideView", "swatch", "amazon", "carousel", "other" ]
    },
    "Type" : {
      "type" : "string",
      "const" : "Tag"
    }
  },
  "type" : "object",
  "properties" : {
    "detail" : {
      "$ref" : "#/$defs/ReviewDetail"
    },
    "summary" : {
      "$ref" : "#/$defs/ReviewSummary"
    }
  }
}