{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "$defs" : {
    "Address" : {
      "type" : "object",
      "properties" : {
        "city" : {
          "type" : "string"
        },
        "company" : {
          "type" : "string"
        },
        "country" : {
          "type" : "string"
        },
        "firstName" : {
          "type" : "string"
        },
        "identifiers" : {
          "type" : "object"
        },
        "lastName" : {
          "type" : "string"
        },
        "phone" : {
          "type" : "string"
        },
        "postal" : {
          "type" : "string"
        },
        "region" : {
          "type" : "string"
        },
        "streets" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        }
      },
      "required" : [ "city", "streets" ]
    },
    "Discount" : {
      "type" : "object",
      "properties" : {
        "amount" : {
          "type" : "number",
          "minimum" : 0
        },
        "code" : {
          "type" : "string"
        },
        "description" : {
          "type" : "string"
        },
        "identifiers" : {
          "type" : "object"
        }
      }
    },
    "LineItem" : {
      "type" : "object",
      "properties" : {
        "children" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/$defs/LineItem"
          }
        },
        "discountAmount" : {
          "type" : "number",
          "minimum" : 0
        },
        "identifiers" : {
          "type" : "object"
        },
        "imageUrls" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "name" : {
          "type" : "string"
        },
        "productIdentifiers" : {
          "type" : "object"
        },
        "quantity" : {
          "type" : "number",
          "exclusiveMinimum" : 0
        },
        "sku" : {
          "type" : "string"
        },
        "subtotal" : {
          "type" : "number",
          "minimum" : 0
        },
        "taxAmount" : {
          "type" : "number",
          "minimum" : 0
        },
        "total" : {
          "type" : "number",
          "minimum" : 0
        },
        "weightInPounds" : {
          "type" : "number",
          "minimum" : 0
        }
      },
      "required" : [ "name", "quantity", "sku", "subtotal", "taxAmount", "total", "weightInPounds" ]
    },
    "Method" : {
      "type" : "string",
      "enum" : [ "CASH", "PO", "CREDIT_CARD", "ON_ACCOUNT" ]
    },
    "Payment" : {
      "type" : "object",
      "properties" : {
        "amount" : {
          "type" : "number",
          "minimum" : 0
        },
        "billToName" : {
          "type" : "string"
        },
        "identifiers" : {
          "type" : "object"
        },
        "method" : {
          "$ref" : "#/$defs/Method"
        },
        "paymentDate" : {
          "type" : "string",
          "format" : "date-time"
        },
        "poNumber" : {
          "type" : "string"
        },
        "status" : {
          "type" : "string"
        }
      },
      "required" : [ "method" ]
    },
    "Provider" : {
      "type" : "string",
      "enum" : [ "FedEx", "UPS", "USPS", "Other" ]
    },
    "ShippingInfo" : {
      "type" : "object",
      "properties" : {
        "accountNumber" : {
          "type" : "string"
        },
        "accountType" : {
          "type" : "string"
        },
        "isBlindShipping" : {
          "type" : "boolean"
        },
        "method" : {
          "type" : "string"
        },
        "provider" : {
          "$ref" : "#/$defs/Provider"
        },
        "weightInPounds" : {
          "type" : "number",
          "minimum" : 0
        }
      },
      "required" : [ "isBlindShipping", "provider" ]
    },
    "Status" : {
      "type" : "string",
      "const" : "CREATED"
    }
  },
  "type" : "object",
  "properties" : {
    "billingAddress" : {
      "$ref" : "#/$defs/Address"
    },
    "comments" : {
      "type" : "string"
    },
    "currencyCode" : {
      "type" : "string"
    },
    "customerIdentifiers" : {
      "type" : "object"
    },
    "discounts" : {
      "type" : "array",
      "items" : {
        "$ref" : "#/$defs/Discount"
      }
    },
    "email" : {
      "type" : "string"
    },
    "firstName" : {
      "type" : "string"
    },
    "identifiers" : {
      "type" : "object"
    },
    "lastName" : {
      "type" : "string"
    },
    "lineItems" : {
      "minItems" : 1,
      "type" : "array",
      "items" : {
        "$ref" : "#/$defs/LineItem"
      }
    },
    "orderDate" : {
      "type" : "string",
      "format" : "date-time"
    },
    "orderNumber" : {
      "type" : "string",
      "description" : "Globally unique orderNumber.  Usually a combination of sales channel and order number",
      "minLength" : 1,
      "maxLength" : 50
    },
    "payments" : {
      "type" : "array",
      "items" : {
        "$ref" : "#/$defs/Payment"
      }
    },
    "salesChannel" : {
      "type" : "string"
    },
    "shippingAddress" : {
      "$ref" : "#/$defs/Address"
    },
    "shippingAmount" : {
      "type" : "number",
      "minimum" : 0
    },
    "shippingInfo" : {
      "$ref" : "#/$defs/ShippingInfo"
    },
    "status" : {
      "$ref" : "#/$defs/Status"
    },
    "subtotal" : {
      "type" : "number",
      "minimum" : 0
    },
    "taxAmount" : {
      "type" : "number",
      "minimum" : 0
    },
    "total" : {
      "type" : "number",
      "minimum" : 0
    }
  },
  "required" : [ "billingAddress", "lineItems", "orderNumber", "salesChannel", "shippingAmount", "subtotal", "taxAmount", "total" ]
}