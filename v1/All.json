{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$defs": {
    "Address": {
      "type": "object",
      "properties": {
        "city": {
          "type": "string"
        },
        "company": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "identifiers": {
          "type": "object"
        },
        "lastName": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "postal": {
          "type": "string"
        },
        "region": {
          "type": "string"
        },
        "streets": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["city", "streets"]
    },
    "AnsweredQuestion": {
      "type": "object",
      "properties": {
        "choices": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "selected": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "type": {
          "$ref": "#/$defs/Type"
        }
      },
      "required": ["choices", "type"]
    },
    "Attribute": {
      "type": "object",
      "properties": {
        "code": {
          "type": "string"
        },
        "displayForCustomer": {
          "type": "boolean"
        },
        "identifiers": {
          "type": "object"
        },
        "label": {
          "type": "string"
        },
        "predefinedValues": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/PredefinedAttributeValue"
          }
        },
        "renderPreference": {
          "$ref": "#/$defs/AttributeRenderPreference"
        },
        "secondarySelectionEnabled": {
          "type": "boolean"
        },
        "usedForProductSelection": {
          "type": "boolean"
        }
      },
      "required": [
        "code",
        "displayForCustomer",
        "label",
        "renderPreference",
        "secondarySelectionEnabled",
        "usedForProductSelection"
      ]
    },
    "AttributeRenderPreference": {
      "type": "string",
      "enum": ["tabs", "dropdown", "swatches", "modal"]
    },
    "Badge": {
      "type": "object",
      "properties": {
        "code": {
          "type": "string"
        },
        "images": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/BadgeImage"
          }
        },
        "priority": {
          "type": "integer"
        },
        "title": {
          "type": "string"
        }
      },
      "required": ["code"]
    },
    "BadgeImage": {
      "type": "object",
      "properties": {
        "context": {
          "$ref": "#/$defs/Context"
        },
        "position": {
          "$ref": "#/$defs/BadgePosition"
        },
        "url": {
          "type": "string"
        }
      },
      "required": ["context", "position", "url"]
    },
    "BadgePosition": {
      "type": "string",
      "enum": ["topRight", "bottomRight", "bottomLeft", "topLeft"]
    },
    "Breadcrumb": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      },
      "required": ["label"]
    },
    "CategoryReference": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "label": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      },
      "required": ["id", "label", "url"]
    },
    "Context": {
      "type": "string",
      "enum": ["product_detail", "product_listing"]
    },
    "Customer": {
      "type": "object",
      "properties": {
        "addresses": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Address"
          }
        },
        "email": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "identifiers": {
          "type": "object"
        },
        "lastName": {
          "type": "string"
        }
      }
    },
    "Discount": {
      "type": "object",
      "properties": {
        "amount": {
          "type": "number",
          "minimum": 0
        },
        "code": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "identifiers": {
          "type": "object"
        }
      }
    },
    "DisplayGroup": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "identifiers": {
          "type": "object"
        },
        "pivots": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Pivot"
          }
        },
        "products": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ProductListing"
          }
        },
        "shoutOuts": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/ShoutOut"
          }
        }
      },
      "required": ["id", "pivots", "products"]
    },
    "LineItem": {
      "type": "object",
      "properties": {
        "children": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/LineItem"
          }
        },
        "discountAmount": {
          "type": "number",
          "minimum": 0
        },
        "identifiers": {
          "type": "object"
        },
        "imageUrls": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "name": {
          "type": "string"
        },
        "productIdentifiers": {
          "type": "object"
        },
        "quantity": {
          "type": "number",
          "exclusiveMinimum": 0
        },
        "sku": {
          "type": "string"
        },
        "subtotal": {
          "type": "number",
          "minimum": 0
        },
        "taxAmount": {
          "type": "number",
          "minimum": 0
        },
        "total": {
          "type": "number",
          "minimum": 0
        },
        "weightInPounds": {
          "type": "number",
          "minimum": 0
        }
      },
      "required": [
        "name",
        "quantity",
        "sku",
        "subtotal",
        "taxAmount",
        "total",
        "weightInPounds"
      ]
    },
    "Media": {
      "type": "object",
      "properties": {
        "altText": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "mediaType": {
          "$ref": "#/$defs/MediaType"
        },
        "previewUrl": {
          "type": "string"
        },
        "tags": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Tag"
          }
        },
        "url": {
          "type": "string"
        }
      },
      "required": ["mediaType", "url"]
    },
    "MediaType": {
      "type": "string",
      "enum": ["image", "video"]
    },
    "MerchantResponse": {
      "type": "object",
      "properties": {
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "identifiers": {
          "type": "object"
        },
        "location": {
          "type": "string"
        },
        "nickname": {
          "type": "string"
        },
        "response": {
          "type": "string"
        },
        "responseType": {
          "$ref": "#/$defs/ResponseType"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "votes": {
          "type": "object"
        }
      }
    },
    "Method": {
      "type": "string",
      "enum": ["CASH", "PO", "CREDIT_CARD", "ON_ACCOUNT"]
    },
    "NonNegativeIntRange": {
      "type": "object",
      "properties": {
        "max": {
          "type": "integer",
          "minimum": 0
        },
        "min": {
          "type": "integer",
          "minimum": 0
        }
      },
      "required": ["min"]
    },
    "Order": {
      "type": "object",
      "properties": {
        "billingAddress": {
          "$ref": "#/$defs/Address"
        },
        "comments": {
          "type": "string"
        },
        "currencyCode": {
          "type": "string"
        },
        "customerIdentifiers": {
          "type": "object"
        },
        "discounts": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Discount"
          }
        },
        "email": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "identifiers": {
          "type": "object"
        },
        "lastName": {
          "type": "string"
        },
        "lineItems": {
          "minItems": 1,
          "type": "array",
          "items": {
            "$ref": "#/$defs/LineItem"
          }
        },
        "orderDate": {
          "type": "string",
          "format": "date-time"
        },
        "orderNumber": {
          "type": "string",
          "description": "Globally unique orderNumber.  Usually a combination of sales channel and order number",
          "minLength": 1,
          "maxLength": 50
        },
        "payments": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Payment"
          }
        },
        "salesChannel": {
          "type": "string"
        },
        "shippingAddress": {
          "$ref": "#/$defs/Address"
        },
        "shippingAmount": {
          "type": "number",
          "minimum": 0
        },
        "shippingInfo": {
          "$ref": "#/$defs/ShippingInfo"
        },
        "status": {
          "$ref": "#/$defs/Status"
        },
        "subtotal": {
          "type": "number",
          "minimum": 0
        },
        "taxAmount": {
          "type": "number",
          "minimum": 0
        },
        "total": {
          "type": "number",
          "minimum": 0
        }
      },
      "required": [
        "billingAddress",
        "lineItems",
        "orderNumber",
        "salesChannel",
        "shippingAmount",
        "subtotal",
        "taxAmount",
        "total"
      ]
    },
    "Payment": {
      "type": "object",
      "properties": {
        "amount": {
          "type": "number",
          "minimum": 0
        },
        "billToName": {
          "type": "string"
        },
        "identifiers": {
          "type": "object"
        },
        "method": {
          "$ref": "#/$defs/Method"
        },
        "paymentDate": {
          "type": "string",
          "format": "date-time"
        },
        "poNumber": {
          "type": "string"
        },
        "status": {
          "type": "string"
        }
      },
      "required": ["method"]
    },
    "Pivot": {
      "type": "object",
      "properties": {
        "attributeRenderPreference": {
          "$ref": "#/$defs/AttributeRenderPreference"
        },
        "code": {
          "type": "string"
        },
        "helpText": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "pivotType": {
          "$ref": "#/$defs/PivotType"
        },
        "values": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/PivotValue"
          }
        }
      },
      "required": ["code", "values"]
    },
    "PivotType": {
      "type": "string",
      "enum": ["primary", "secondary"]
    },
    "PivotValue": {
      "type": "object",
      "properties": {
        "helpText": {
          "type": "string"
        },
        "label": {
          "type": "string"
        },
        "products": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "swatchType": {
          "$ref": "#/$defs/SwatchType"
        },
        "swatchValue": {
          "type": "string"
        }
      },
      "required": ["label", "products"]
    },
    "PredefinedAttributeValue": {
      "type": "object",
      "properties": {
        "identifiers": {
          "type": "object"
        },
        "swatches": {
          "type": "object"
        },
        "value": {
          "type": "string"
        }
      },
      "required": ["identifiers", "value"]
    },
    "Product": {
      "type": "object",
      "properties": {
        "badges": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "brand": {
          "type": "string"
        },
        "browsable": {
          "type": "boolean"
        },
        "content": {
          "$ref": "#/$defs/ProductContent"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "customization": {
          "$ref": "#/$defs/ProductCustomization"
        },
        "displayGroup": {
          "type": "string"
        },
        "identifiers": {
          "type": "object"
        },
        "media": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Media"
          }
        },
        "name": {
          "type": "string"
        },
        "primaryCategory": {
          "$ref": "#/$defs/CategoryReference"
        },
        "purchaseOptions": {
          "$ref": "#/$defs/PurchaseOptions"
        },
        "searchable": {
          "type": "boolean"
        },
        "sku": {
          "type": "string"
        },
        "specs": {
          "type": "object"
        },
        "status": {
          "type": "string"
        },
        "taxonomy": {
          "$ref": "#/$defs/Taxonomy"
        },
        "upc": {
          "type": "string"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "url": {
          "minItems": 1,
          "type": "array",
          "items": {
            "$ref": "#/$defs/ProductUrl"
          }
        }
      },
      "required": [
        "browsable",
        "name",
        "searchable",
        "sku",
        "specs",
        "taxonomy",
        "url"
      ]
    },
    "ProductContent": {
      "type": "object",
      "properties": {
        "breadcrumbs": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Breadcrumb"
          }
        },
        "longDescription": {
          "type": "string"
        },
        "metaDescription": {
          "type": "string"
        },
        "metaKeywords": {
          "type": "string"
        },
        "metaTitle": {
          "type": "string"
        },
        "seoDescription": {
          "type": "string"
        },
        "shortDescription": {
          "type": "string"
        },
        "shortName": {
          "type": "string"
        }
      }
    },
    "ProductCustomization": {
      "type": "object",
      "properties": {
        "prebuiltConfigurationId": {
          "type": "string"
        },
        "prebuiltCustomizable": {
          "type": "boolean"
        },
        "prebuiltDesign": {
          "type": "boolean"
        },
        "stockType": {
          "type": "string"
        },
        "styleType": {
          "type": "string"
        }
      }
    },
    "ProductListing": {
      "type": "object",
      "properties": {
        "isCustomizable": {
          "type": "boolean"
        },
        "mainImageUrl": {
          "type": "string"
        },
        "pivotValues": {
          "type": "object"
        },
        "shortName": {
          "type": "string"
        },
        "sku": {
          "type": "string"
        }
      },
      "required": ["isCustomizable", "sku"]
    },
    "ProductReview": {
      "type": "object",
      "properties": {
        "detail": {
          "$ref": "#/$defs/ReviewDetail"
        },
        "summary": {
          "$ref": "#/$defs/ReviewSummary"
        }
      }
    },
    "ProductUrl": {
      "type": "object",
      "properties": {
        "metaData": {
          "$ref": "#/$defs/ProductUrlMetaData"
        },
        "url": {
          "type": "string"
        },
        "urlType": {
          "$ref": "#/$defs/ProductUrlType"
        }
      }
    },
    "ProductUrlMetaData": {
      "type": "object",
      "properties": {
        "creativeEngine": {
          "$ref": "#/$defs/ProductUrlMetaDataCreativeEngine"
        },
        "designer": {
          "$ref": "#/$defs/ProductUrlMetaDataDesigner"
        }
      }
    },
    "ProductUrlMetaDataCreativeEngine": {
      "type": "string",
      "enum": ["chili", "alchemy"]
    },
    "ProductUrlMetaDataDesigner": {
      "type": "string",
      "enum": ["chili", "luma", "addrLogo"]
    },
    "ProductUrlType": {
      "type": "string",
      "enum": ["product", "customProduct", "yourLogoHere"]
    },
    "Provider": {
      "type": "string",
      "enum": ["FedEx", "UPS", "USPS", "Other"]
    },
    "PurchaseOptions": {
      "type": "object",
      "properties": {
        "allowsSample": {
          "type": "boolean"
        },
        "customLeadTimeDayRange": {
          "$ref": "#/$defs/NonNegativeIntRange"
        },
        "customizable": {
          "type": "boolean"
        },
        "designRequired": {
          "type": "boolean"
        },
        "inStock": {
          "type": "boolean"
        },
        "isReturnable": {
          "type": "boolean"
        },
        "maxPrice": {
          "type": "number"
        },
        "minCustomizationQty": {
          "type": "integer",
          "exclusiveMinimum": 0
        },
        "minPrice": {
          "type": "number"
        },
        "minSaleQty": {
          "type": "integer",
          "exclusiveMinimum": 0
        },
        "onSale": {
          "type": "boolean"
        },
        "plainLeadTimeDayRange": {
          "$ref": "#/$defs/NonNegativeIntRange"
        },
        "pricePercentOff": {
          "type": "number"
        },
        "quantityStepIncrements": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/QuantityStepIncrement"
          }
        },
        "stockQty": {
          "type": "integer",
          "minimum": 0
        },
        "suggestedQuantityDisplays": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        },
        "tierPrices": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/TierPrice"
          }
        },
        "toBeDiscontinued": {
          "type": "boolean"
        }
      },
      "required": [
        "allowsSample",
        "customizable",
        "designRequired",
        "inStock",
        "isReturnable",
        "maxPrice",
        "minPrice",
        "stockQty",
        "tierPrices",
        "toBeDiscontinued"
      ]
    },
    "QuantityStepIncrement": {
      "type": "object",
      "properties": {
        "requiredStepIncrement": {
          "type": "integer"
        },
        "startingAtQty": {
          "type": "integer"
        }
      },
      "required": ["requiredStepIncrement", "startingAtQty"]
    },
    "ResponseType": {
      "type": "string",
      "const": "merchantResponse"
    },
    "ReviewDetail": {
      "type": "object",
      "properties": {
        "answeredQuestions": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/AnsweredQuestion"
          }
        },
        "comments": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "identifiers": {
          "type": "object"
        },
        "location": {
          "type": "string"
        },
        "maxRating": {
          "type": "number"
        },
        "media": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Media"
          }
        },
        "merchantResponses": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/MerchantResponse"
          }
        },
        "nickname": {
          "type": "string"
        },
        "rating": {
          "type": "number"
        },
        "sku": {
          "type": "string"
        },
        "source": {
          "$ref": "#/$defs/ReviewSource"
        },
        "title": {
          "type": "string"
        },
        "votes": {
          "type": "object"
        }
      },
      "required": ["sku"]
    },
    "ReviewSource": {
      "type": "string",
      "const": "email"
    },
    "ReviewSummary": {
      "type": "object",
      "properties": {
        "displayGroupId": {
          "type": "integer"
        },
        "maxRating": {
          "type": "number"
        },
        "rating": {
          "type": "number"
        },
        "reviewCount": {
          "type": "integer"
        },
        "sku": {
          "type": "string"
        }
      }
    },
    "Shipment": {
      "type": "object",
      "properties": {
        "carrier": {
          "type": "string"
        },
        "comments": {
          "type": "string"
        },
        "currencyCode": {
          "type": "string"
        },
        "estimatedDeliveryDate": {
          "type": "string",
          "format": "date-time"
        },
        "identifiers": {
          "type": "object"
        },
        "lineItems": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/LineItem"
          }
        },
        "orderIdentifiers": {
          "type": "object"
        },
        "orderNumber": {
          "type": "string",
          "description": "Globally unique orderNumber.  Usually a combination of sales channel and order number",
          "minLength": 1,
          "maxLength": 50
        },
        "shipmentDate": {
          "type": "string",
          "format": "date-time"
        },
        "trackingNumber": {
          "type": "string"
        },
        "trackingUrl": {
          "type": "string"
        }
      },
      "required": ["orderNumber", "shipmentDate"]
    },
    "ShippingInfo": {
      "type": "object",
      "properties": {
        "accountNumber": {
          "type": "string"
        },
        "accountType": {
          "type": "string"
        },
        "isBlindShipping": {
          "type": "boolean"
        },
        "method": {
          "type": "string"
        },
        "provider": {
          "$ref": "#/$defs/Provider"
        },
        "weightInPounds": {
          "type": "number",
          "minimum": 0
        }
      },
      "required": ["isBlindShipping", "provider"]
    },
    "ShoutOut": {
      "type": "object",
      "properties": {
        "display": {
          "type": "string"
        },
        "type": {
          "$ref": "#/$defs/ShoutOutType"
        },
        "url": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": ["display", "type", "url"]
    },
    "ShoutOutType": {
      "type": "string",
      "enum": ["generic", "product", "category", "group", "similar"]
    },
    "Status": {
      "type": "string",
      "const": "CREATED"
    },
    "SwatchType": {
      "type": "string",
      "enum": ["TEXT", "IMAGE", "RGB"]
    },
    "Tag": {
      "type": "string",
      "enum": [
        "primary",
        "small",
